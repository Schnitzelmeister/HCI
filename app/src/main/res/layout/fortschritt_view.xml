<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.uniwien.gesunderleben.MainActivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Fortschritt"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.043" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="71dp"
        android:layout_height="62dp"
        app:srcCompat="@mipmap/distance_icon"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="400dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="71dp"
        android:layout_height="62dp"
        app:srcCompat="@mipmap/tempo_icon"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="200dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="71dp"
        android:layout_height="62dp"
        app:srcCompat="@mipmap/time_icon"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="300dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="71dp"
        android:layout_height="62dp"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        app:srcCompat="@mipmap/weight_icon"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="100dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="242dp"
        android:layout_height="46dp"
        android:textColor="#008000"
        android:text="Durchschn. Distanz (neu): 12,74 Km"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="400dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="251dp"
        android:layout_height="43dp"
        android:textColor="#008000"
        android:text="Durchschn. Laufzeit (neu): 80 Minuten"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="300dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="252dp"
        android:layout_height="47dp"
        android:textColor="#008000"
        android:text="Durchschn. Lauftempo (neu): 9,8 Km/h"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="200dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="241dp"
        android:layout_height="35dp"
        android:textColor="#ff0000"
        android:text="Durchschn. Distanz (alt): 10,98 Km"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="447dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="250dp"
        android:layout_height="35dp"
        android:text="Durchschn. Laufzeit (alt): 75 Minuten"
        android:textColor="#ff0000"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="347dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="252dp"
        android:layout_height="36dp"
        android:text="Durchschn. Lauftempo (alt): 8,78 Km/h"
        android:textColor="#ff0000"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="247dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="194dp"
        android:layout_height="39dp"
        android:text="Gewicht (alt): 85 Kg"
        android:textColor="#ff0000"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="147dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="206dp"
        android:layout_height="47dp"
        android:text="Gewicht (neu): 78 Kg"
        android:textColor="#008000"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="100dp" />

</android.support.constraint.ConstraintLayout>
